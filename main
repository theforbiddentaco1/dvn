exploitTab:AddBind({
	Name = "15 SEC INVINCIBLE",
	Default = Enum.KeyCode.V,
	Hold = false,
	Callback = function()
OrionLib:MakeNotification({
	Name = "started!",
	Content = "invincible for 10s",
	Image = "rbxassetid://4483345998",
	Time = 3
})
--- 15 SEC HEAL/INVINC
local toolNames = {"Medical Bow", "Recurve", "Vitabow"}
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local CollectionService = game:GetService("CollectionService")
local toolNameDict = {}
for _, name in ipairs(toolNames) do toolNameDict[name] = true end
local function fireVerifyHit(tool)
local a1 = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
if not a1 then return end local a2 = Vector3.zero
local a3 = Vector3.new(0, 0, 10000) local meow = tool.Parent
local toolInstance = meow:FindFirstChild(tool.Name)
if toolInstance and toolInstance:FindFirstChild("VerifyHit") then
toolInstance.VerifyHit:FireServer(a1, a2, a3) end end local function tagTools(tool)
if tool:IsA("Tool") and toolNameDict[tool.Name] then
CollectionService:AddTag(tool, "TrackedTool") fireVerifyHit(tool) end end
local function untagTools(tool) if CollectionService:HasTag(tool, "TrackedTool") then
CollectionService:RemoveTag(tool, "TrackedTool") end end local function trackWorkspaceTools()
for _, bow in ipairs(workspace:GetDescendants()) do tagTools(bow) end end
local function trackPlayerTools(player) for _, descendant in ipairs(player:GetDescendants()) do
tagTools(descendant) end end workspace.DescendantAdded:Connect(tagTools)
workspace.DescendantRemoving:Connect(untagTools) Players.PlayerAdded:Connect(function(player)
player.DescendantAdded:Connect(tagTools) player.DescendantRemoving:Connect(untagTools)
player.CharacterAdded:Connect(function(character) trackPlayerTools(player)
end) end) trackWorkspaceTools() for _, player in pairs(Players:GetPlayers()) do
trackPlayerTools(player) end local startTime = tick()
local heartbeatConnection

heartbeatConnection = RunService.Heartbeat:Connect(function(step)
    local currentTime = tick() -- Get the current time in each loop
    if currentTime - startTime >= 10 then
    print("5 seconds passed, stopping tool tracking.")
    OrionLib:MakeNotification({
	Name = "it's over",
	Content = "it's done do it again brah",
	Image = "rbxassetid://4483345998",
	Time = 3
})
    heartbeatConnection:Disconnect()
    return end
trackWorkspaceTools()
for _, player in pairs(Players:GetPlayers()) do
trackPlayerTools(player)
end end)
  	end    
})

exploitTab:AddBind({
	Name = "everything",
	Default = Enum.KeyCode.P,
	Hold = false,
	Callback = function()
local lp = game.Players.LocalPlayer.Character
local Weapon = lp:FindFirstChildOfClass("Tool")
local validWeapons = { "SMG", "Twinface","Bolter", "Harpoon Gun", "Rifle", "Vitabow", "Recurve", "Voltaic Impact","Gunslingers","Akimbo","Armour Peeler","Gift of Fire", "DMR","Stonewall", "Steelforge", "Burst Rifle", "Shotgun", "Sprinter's Streak", "Hallsweeper"}

if table.find(validWeapons, Weapon.Name) then
OrionLib:MakeNotification({ Name = "weapon modified!", Content = "true", Image = "rbxassetid://4483345998", Time = 1.4 })
Weapon:GetAttributeChangedSignal("Ammo"):Connect( function()
Weapon:SetAttribute("Ammo", 500) end)
Weapon:SetAttribute("BulletSpeed", 9999) Weapon:SetAttribute("Spread", 0)
Weapon:SetAttribute("BurstCount", 10) Weapon:SetAttribute("ClipSize", 501) end

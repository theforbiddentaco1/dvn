-- SPECIALIST TAB
local specialistTab = Window:MakeTab({
	Name = "specialist",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
--- AUTO KILL BOSSES
local lp = game.Players.LocalPlayer local char = lp.Character local backpack = lp.Backpack
local damagePerHitDefault = 101 local damagePerHitHermes = 20 local autoKillEnabled = false local connection
local enemiesToTarget = {
    "Trident", 
    "Tempest", 
    "Fusilier", 
    "Gaia", 
    "Hermes", 
    "Drone", 
    "Achilles", 
    "Sparta", 
    "Launcher1", 
    "Launcher2", 
    "Launcher3", 
    "Launcher4",
    "Ares"
}
local function getWeapon() return char:FindFirstChild("Torqueblade") or backpack:FindFirstChild("Torqueblade") end
local function fireRemoteEvent(enemy) local Weapon = getWeapon() if Weapon then local a1 = Vector3.new(0, 0, 0) local a3 = "meow"
local health = enemy.Health local timesToRun if enemy.Parent.Name == "Hermes" then timesToRun = math.ceil(health / damagePerHitHermes) else
timesToRun = math.ceil(health / damagePerHitDefault) end for i = 1, timesToRun do Weapon.VerifyThrowHit:FireServer(a1, enemy, a3) end end end
local function detectEnemies() connection = workspace.DescendantAdded:Connect(function(child) if autoKillEnabled then if child:IsA("Humanoid") and child.Health > 0 then
local enemy = child.Parent for _, enemyName in pairs(enemiesToTarget) do if enemy.Name == enemyName then spawn(function() fireRemoteEvent(child) end) break end end end end
end) for _, enemy in pairs(workspace:GetDescendants()) do if enemy:IsA("Humanoid") and enemy.Health > 0 then
local enemyParent = enemy.Parent for _, enemyName in pairs(enemiesToTarget) do if enemyParent.Name == enemyName then fireRemoteEvent(enemy) break end end
end end end
specialistTab:AddToggle({ Name = "auto kill bosses", Default = false, Callback = function(Value) autoKillEnabled = Value
if Value then detectEnemies() else if connection then connection:Disconnect() connection = nil end end
end
}) coroutine.wrap(function() while true do wait(1) getWeapon() end end)() -- Keep checking for Torqueblade
